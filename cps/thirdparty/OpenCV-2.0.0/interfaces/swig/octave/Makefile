clean-libtool:
	-rm -rf .libs _libs
install-nodist_octaveDATA: $(nodist_octave_DATA)
	@$(NORMAL_INSTALL)
	test -z "$(octavedir)" || $(MKDIR_P) "$(DESTDIR)$(octavedir)"
	@list='$(nodist_octave_DATA)'; test -n "$(octavedir)" || list=; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(octavedir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(octavedir)" || exit $$?; \
	done

uninstall-nodist_octaveDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(nodist_octave_DATA)'; test -n "$(octavedir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	test -n "$$files" || exit 0; \
	echo " ( cd '$(DESTDIR)$(octavedir)' && rm -f" $$files ")"; \
	cd "$(DESTDIR)$(octavedir)" && rm -f $$files
